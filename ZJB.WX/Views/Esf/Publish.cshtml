            <input type="hidden" name="chouseId" id="chouseId" value="0">
            <input type="hidden" name="isChange" id="isChange" value="1">
            <input type="hidden" name="closeTip" id="closeTip" value="0">
            <input type="hidden" name="currentTimeMillis" id="currentTimeMillis" value="">
            <input type="hidden" name="draftType" id="draftType" value="">
            <input type="hidden" id="cellCode" name="cellCode" value="" />
            <input type="hidden" id="distrct" name="distrct" value="" />
            <input type="hidden" id="room" name="room" value="" />
            <input type="hidden" id="hall" name="hall" value="" />
            <input type="hidden" id="toilet" name="toilet" value="" />
            <input type="hidden" id="fiveYears" name="fiveYears" value="0" />
            <input type="hidden" id="onlyHouse" name="onlyHouse" value="0" />
            <input type="hidden" id="addr" name="addr" value="" />

            <input type="hidden" id="pointTo" name="pointTo" value="" />
            <input type="hidden" id="fitmentStatus" name="fitmentStatus" value="" />

            <ul class="elements step-one">
                <li class="element">
                    <p class="element-title">所在城市</p>
                    <dl class="element-panel select-panel-dl">
                        <dt>城市</dt>
                        <dd>
                            <select name="type" disabled="disabled" class="fabu_select">
                                <option value="1">厦门</option>
                                <!--<option value="2">北京</option>-->
                            </select>
                        </dd>
                    </dl>
                </li>
                <li class="element owner-info">
                    <p class="element-title">业主信息</p>
                    <dl class="element-panel">
                        <dt>姓名</dt>
                        <dd>
                            <input type="text" id="contacts" placeholder="输入业主姓名" class="estate-info-name" name="contacts" style="width: 2rem">

                        </dd>
                    </dl>
                    <dl class="element-panel">
                        <dt>电话</dt>
                        <dd class="element-gray">
                            <input type="text" placeholder="电话" id="tel" name="tel" value="@ViewBag.Tel" readonly="true" class="estate-info-name" style="width: 2rem">
                            <input type="hidden" class="estate-value" value="@ViewBag.Tel">
                            @*<span class="owner-info-telshow">186****6926</span>
                                <input type="hidden" class="owner-info-tel" value="18650006926">*@
                        </dd>
                    </dl>
                </li>
                <li class="element estate-info">
                    <p class="element-title">小区信息</p>
                    <dl class="element-panel estate-info-name-dl">
                        <dt>名称</dt>
                        <dd>
                            <input type="text" placeholder="输入小区名称" id="cell" name="cell" class="estate-info-name" autocomplete="off" style="width: 2rem">
                            <input type="hidden" class="estate-value" value="">


                        </dd>
                    </dl>
                    <dl class="element-panel select-panel-dl needsclick">
                        <dt>地址</dt>
                        <span class="select-panel estate-info-addr" id="txt_address" disabled="" style="width: 2.2rem;" data-list="" data-selval=""><i class="place-holder">选择小区地址</i></span>
                    </dl>


                </li>
            </ul>
            <ul class="elements step-two" style="display: ">
                <li class="element house-info">
                    <p class="element-title">房源信息</p>

                    <dl id="unit_number" class="element-panel select-panel-dl">
                        <dt>楼层</dt>
                        <dd>
                            <div class="unitbox house-info-dyh" data-use-input="true" data-paclass="pa-dyh" data-maxlen="6" data-inputname="buildingDyh" data-unitname="层" data-placeholder="输入所在楼层">
                                <input id="curFloor" type="number" name="curFloor" class="input-box" maxlength="6" placeholder="输入所在楼层" autocomplete="off" value="" rule="" reg="" minvalue="" maxvalue="" msg="">
                                <span class="input-box-span"></span><span class="place-holder">输入所在楼层</span><span class="unit"></span>
                            </div>
                        </dd>
                    </dl>
                    <dl id="unit_number" class="element-panel select-panel-dl">
                        <dt>总层</dt>
                        <dd>
                            <div class="unitbox house-info-dyh" data-use-input="true" data-paclass="pa-dyh" data-maxlen="6" data-inputname="buildingDyh" data-unitname="层" data-placeholder="输入总楼层">
                                <input id="maxFloor" type="number" name="maxFloor" class="input-box" maxlength="6" placeholder="输入总楼层" autocomplete="off" value="" rule="" reg="" minvalue="" maxvalue="" msg="">
                                <span class="input-box-span"></span><span class="place-holder">输入总楼层</span><span class="unit">层</span>
                            </div>
                        </dd>
                    </dl>
                    <dl class="element-panel select-panel-dl" id="huxing-select">
                        <dt>户型</dt>
                        <dd><span class="select-panel house-info-hx"><i class="place-holder pleft">选择户型</i></span> </dd>
                    </dl>
                    <dl class="element-panel">
                        <dt>面积</dt>
                        <dd>
                            <div class="unitbox house-info-mj" data-use-input="true" data-inputtype="number" data-maxlen="7" data-inputname="houseArea" data-unitname="平" data-placeholder="输入面积">
                                <input type="number" id="houseArea" name="houseArea" class="input-box" min="" max="" placeholder="输入面积" autocomplete="off" value="" rule="" reg="" minvalue="" maxvalue="" msg="">
                                <span class="input-box-span"></span><span class="place-holder">输入面积</span><span class="unit">平</span>
                            </div>
                        </dd>
                    </dl>

                    <dl class="element-panel select-panel-dl" id="zx-select">
                        <dt>装修</dt>
                        <dd> <span class="select-panel house-info-hx"><i class="place-holder pleft">选择装修</i></span> </dd>
                    </dl>
                    <dl class="element-panel select-panel-dl" id="cx-select">
                        <dt>朝向</dt>
                        <dd> <span class="select-panel house-info-hx"><i class="place-holder pleft">选择朝向</i></span> </dd>
                    </dl>
                    <dl class="element-panel">
