    <div id="dt-login-cs" class="dt-login-layer js-action-dt-login-layer js-action-nomove" style="display: none;"></div>
    <script language="javascript">
        var huxing = ""; //记录选择的户型
        var cx = ""; //记录选择的朝向
        var zx = ""; //记录选择的装修类型
        function showProtocol() {
            $("#closep").show();
            $("#Protocol").show();
            $(".mod-delegate-submit-panel").hide();
            $(".button-sub").hide();
            $("#hidewt").show();
        }

        function closeProtocol() {
            $("#Protocol").hide();
            $(".mod-delegate-submit-panel").show();
            $(".button-sub").show();
            $("#closep").hide();
            $("#hidewt").hide();

        }

        $(function () {
            var s = $(".elements-buttons .button"), l = $("body").height(), o = s.height();
            $(".mod-delegate-submit-panel").css("height", l - o);

            //是否满五
            $(".fiveYears-info").bind("click", function () {
                if ($(this).attr("data-value") == 0) {
                    $(this).addClass("right");
                    $(this).attr("data-value", "1");
                } else {
                    $(this).removeClass("right");
                    $(this).attr("data-value", "0");
                }
                $("#fiveYears").val($(this).attr("data-value"));
            });

            //是否唯一住房
            $(".onlyHouse-info").bind("click", function () {
                if ($(this).attr("data-value") == 0) {
                    $(this).addClass("right");
                    $(this).attr("data-value", "1");
                } else {
                    $(this).removeClass("right");
                    $(this).attr("data-value", "0");
                }
                $("#onlyHouse").val($(this).attr("data-value"));
            });


            //小区名称弹出层
            $(".estate-info-name-dl").bind("click", function () {
                $("#xiaoqu-sr").addClass("active");
                $("input[name=estateSearch]").focus();
            });
            $(".search-panel-cancel").bind("click", function () {
                $("#xiaoqu-sr").removeClass("active");
                $("input[name=estateSearch]").val("");
                $(".search-close").hide();
                $(".hint-wrap").hide()
            });
            $("input[name=estateSearch]").bind("keyup", function () {
                if ($(this).val() == "") {
                    $(".search-close").hide();
                } else {
                    $.ajax({
                        type: "post",
                        url: "/House/GetCellsByInput",
                        data: "buildingType=1&inputStr=" + $("input[name=estateSearch]").val(),
                        dataType: "json",
                        success: function (data) {
                            var cells = data.cells;
                            $(".search-close").show();
                            $(".hint-wrap").show();
                            var xq = "";
                            $.each(cells, function (i, n) {
                                //$(".keyList").append("<dd><a index='" + i + "'>" + n.cellName + "</a></dd>");
                                xq += "<a onclick=\"xqinfo(this,'" + n.address + "'," + n.cellCode + "," + n.district + ")\" class=\"hint-item\" data-id=\"YuNJt8eiNyo\" data-key=\"" + n.cellName + "\" data-kw=\"" + n.cellName + "\">";
                                xq += "  <em class=\"iconfont\">혐</em> <span class=\"txt-wrap\">";
                                xq += "     <span class=\"key-txt\">" + n.cellName + "</span>";
                                xq += "   <span class=\"kw-txt\">" + n.address + "</span>";
                                xq += "  </span>";
                                xq += "</a>";

                            });
                            $(".hint-wrap").html(xq);

                            //$(".keyList").find("dd").eq(0).children("a").addClass("upordownSelect");
                            //$(".keyList").animate({ scrollTop: 0 }, 100);
                            //j = 0;
                            //top = 0;
                            //scrollNum = 0;
                        }
                    });

                }
            });
            $(".search-close").bind("click", function () {
                $(this).hide();
                $(".hint-wrap").hide();
                $("input[name=estateSearch]").val("");
            });
            //$(".hint-wrap a").bind("click", function () {
            //    $("input[name=cell]").val($(this).attr("data-key"));
            //    $(".place-holder").html($(this).attr("data-kw"));
            //    $(".search-close").hide();
            //    $("#xiaoqu-sr").removeClass("active");
            //    $(".hint-wrap").hide();
            //    $("input[name=estateSearch]").val("");
            //});

            //点击下一步
            $(".button-next").bind("click", function () {
                if ($("input[name=contacts]").val() == "") {
                    $("body").prepend(createDiv("请输入您的真实姓名"));
                    var k = setInterval(function () {
                        $(".mod-smallnote").remove();
                        clearInterval(k);
                    }, 1000);
                } else if ($("input[name=cell]").val() == "") {
                    $("body").prepend(createDiv("请选择小区"));
                    var k = setInterval(function () {
                        $(".mod-smallnote").remove();
                        clearInterval(k);
                    }, 1000);
                } else {
                    $(".step-one").hide();
                    $(".step-two").show();
                    $(this).hide();
                    $(".button-sub").show()


                }
            });

            //提交房源
            $(".button-sub").bind("click", function () {



                $("#dt-login-cs").show();
                var priceTip = '@(posttype == 1 ? "售价" : "租金")';
                if ($("input[name=contacts]").val() == "") {
                    $("body").prepend(createDiv("请输入您的真实姓名"));
                    var k = setInterval(function () {
                        $(".mod-smallnote").remove();
                        $("#dt-login-cs").hide();
                        clearInterval(k);
                    }, 1000);
                } else if ($("input[name=cell]").val() == "") {
                    $("body").prepend(createDiv("请选择小区"));
                    var k = setInterval(function () {
                        $(".mod-smallnote").remove();
                        $("#dt-login-cs").hide();
                        clearInterval(k);
                    }, 1000);
                }  else if ($("input[name=houseArea]").val() == "") {
                    $("body").prepend(createDiv("请输入面积"));
                    var k = setInterval(function () {
                        $(".mod-smallnote").remove();
                        $("#dt-login-cs").hide();
                        clearInterval(k);
                    }, 1000);
                } else if ($("input[name=price]").val() == "") {
                    $("body").prepend(createDiv("请输入" + priceTip));
                    var k = setInterval(function () {
                        $(".mod-smallnote").remove();
                        $("#dt-login-cs").hide();
                        clearInterval(k);
                    }, 1000);
                }

                else if ($("#fitmentStatus").val() == "") {
                    $("body").prepend(createDiv("请选择装修！"));
                    var k = setInterval(function () {
                        $(".mod-smallnote").remove();
                        $("#dt-login-cs").hide();
                        clearInterval(k);
                    }, 1000);
                }
                else if ($("#pointTo").val() == "") {
                    $("body").prepend(createDiv("请选择朝向！"));
                    var k = setInterval(function () {
                        $(".mod-smallnote").remove();
                        $("#dt-login-cs").hide();
                        clearInterval(k);
                    }, 1000);
                }


                else if ($("input[name=title]").val() == "") {
                    $("body").prepend(createDiv("房源卖点不能为空！"));
                    var k = setInterval(function () {
                        $(".mod-smallnote").remove();
                        $("#dt-login-cs").hide();
                        clearInterval(k);
                    }, 1000);
                }
                else if ($("input[name=houseDescribe]").val() == "") {
                    $("body").prepend(createDiv("详细描述不能为空！"));
                    var k = setInterval(function () {
                        $(".mod-smallnote").remove();
                        $("#dt-login-cs").hide();
                        clearInterval(k);
                    }, 1000);
                }



                else {
                    $("body").prepend(createDiv("正在发布中"));
                    var k = setInterval(function () {
                        $(".mod-smallnote").remove();
                        $("#dt-login-cs").hide();
                        clearInterval(k);
                    }, 3000);
                    $("#bForm").submit();

                }
            });

            //选择楼号、室号等
            $(".element-panel i").bind("click", function () {
                $(this).parent().find("p").toggle()
            });
            $("i.tip-no-info-btn").bind("click", function () {
                $(this).parent().hide();
                $(this).parent().parent().parent().find("input").val($(this).html());
            });
            $(".input-box").bind("keyup", function () {
                if ($(this).val() != "") {
                    $(this).parent().find(".unit").show()
                }
            });

            //选择购房日期、是否唯一房产
            //$("#triggerproperty").bind("click", function () {
            //    $("#mask").show();
            //    $("#select_timer").addClass("tab");
            //});
            //$("#triggeronly").bind("click", function () {
            //    $("#mask").show();
            //    $("#select_number").addClass("tab");
            //});
            $("#select_timer a").bind("click", function () {
                if ($(this).attr("title") == "0") {
                    $("#select_timer").removeClass("tab");
                    $("#mask").hide();
                } else {
                    $("#triggerproperty").find("span").html($(this).html());
                    $("#select_timer").removeClass("tab");
                    $("#mask").hide();
                }
            });
            $("#select_number a").bind("click", function () {
                if ($(this).attr("title") == "0") {
                    $("#select_number").removeClass("tab");
                    $("#mask").hide();
                } else {
                    $("#triggeronly").find("span").html($(this).html());
                    $("#select_number").removeClass("tab");
                    $("#mask").hide();
                }
            });

            //户型选择
            $("#huxing-select").bind("click", function () {
                $("#huxingShiDrap").show();
                huxing = "";
            });


            $("#zx-select").bind("click", function () {
                $("#zxShiDrap").show();

            });
            $("#cx-select").bind("click", function () {
                $("#cxShiDrap").show();

            });



            $("#huxingShiDrapCon li").bind("click", function () {
                huxing += $(this).html();
                $("#room").val($(this).attr("data-id"));
                $("#huxing-select i").html(huxing);
                $("#huxingTingText").html(huxing);
                $("#huxingShiDrap").hide();
                $("#huxingTingDrap").show();


            });
            $("#huxingTingDrapCon li").bind("click", function () {
                huxing += $(this).html();
                $("#hall").val($(this).attr("data-id"));
                $("#huxing-select i").html(huxing);
                $("#huxingWeiText").html(huxing);
                $("#huxingShiDrap").hide();
                $("#huxingTingDrap").hide();
                $("#huxingWeiDrap").show()
            });
            $("#huxingWeiDrapCon li").bind("click", function () {
                huxing += $(this).html();
                $("#toilet").val($(this).attr("data-id"));
                $("#huxing-select i").html(huxing);
                $("#huxingShiDrap").hide();
                $("#huxingTingDrap").hide();
                $("#huxingWeiDrap").hide()
            });
            $(".cancel").bind("click", function () {
                $("#huxingShiDrap").hide();
                $("#huxingTingDrap").hide();
                $("#huxingWeiDrap").hide()

                $("#zxShiDrap").hide()
                $("#cxShiDrap").hide()
            })

            $("#zxShiDrapCon li").bind("click", function () {
                $("#fitmentStatus").val($(this).html());
                $("#zx-select i").html($(this).html());
                $("#zxShiDrap").hide()
            });
            $("#cxShiDrapCon li").bind("click", function () {
                $("#pointTo").val($(this).html());
                $("#cx-select i").html($(this).html());
                $("#cxShiDrap").hide()
            });

            //协议
            $(".element-agree em").bind("click", function () {
                if ($(this).parent().hasClass("checked")) {
                    $(this).parent().removeClass("checked");
                    $(".button-sub").addClass("disabled");
                } else {
                    $(this).parent().addClass("checked");
                    $(".button-sub").removeClass("disabled");
                }
            });
        })

        function xqinfo(obj, address, communityid, district) {
            $("input[name=cell]").val($(obj).attr("data-key"));
            $("#txt_address").html(address);

            $("#addr").val(address);
            $(".search-close").hide();
            $("#xiaoqu-sr").removeClass("active");

            $("#cellCode").val(communityid);
            $("#distrct").val(district);

            $(".hint-wrap").hide();
            $("input[name=estateSearch]").val("");
        }


        $(window).resize(function () {
            var s = $(".elements-buttons .button"), l = $("body").height(), o = s.height();
            $(".mod-delegate-submit-panel").css("height", l - o);
        })
    </script>
    @{ Html.RenderPartial("Cnzz"); }
