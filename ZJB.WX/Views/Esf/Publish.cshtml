                        <dl class="element-panel">
                            <dt>租金</dt>
                            <dd>
                                <div class="unitbox house-price" data-use-input="true" data-inputtype="number" min="1" max="999999" data-inputname="price" data-unitname="元/月" data-placeholder="输入租金">
                                    <input type="number" name="price" class="input-box" min="" max="" placeholder="输入租金" autocomplete="off" value="" rule="" reg="" minvalue="" maxvalue="" msg="">
                                    <span class="input-box-span"></span><span class="place-holder">输入租金</span><span class="unit">元/月</span>
                                </div>
                            </dd>
                        </dl>
                    </li>
                }
                <li class="element-agree">
                    <label class="mod-check-box sub-agree checked">
                        <em class="mycheckicon"></em>
                        <input type="radio" class="house-ts-agree">
                    </label>
                    <b>同意<a class="protol-link" onclick="showProtocol()">《在线委托协议》</a></b>
                </li>
            </ul>
        </form>
    </div>
    <div class="elements-buttons">
        @*<p class="button button-next">下一步</p>*@
        <p class="button button-sub " @*style="display: none;"*@>提交房源</p>
