                    $(".all-body").removeClass("modal-open");
                    $(".filter-district").hide();
                    $(".filter-subway").hide();
                })
            });
            $("div[data-filter-choose-type=subway]").bind("click", function (e) {
                $(".filter-district").hide();
                $(".filter-subway").show();
