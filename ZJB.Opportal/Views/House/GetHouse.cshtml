@if (isCloned == 1)
{


    <head>
        <link href="~/Css/common.css" rel="stylesheet" />
        <style type="text/css">
            .footer-tip {
                color: green;
                font-size: 20px;
                font-weight: bold;
                margin: 150px auto;
                text-align: center;
            }

                .footer-tip li {
                    margin: 20px 0;
                }

                .footer-tip a:hover {
                    text-decoration: underline;
                }
        </style>
    </head>
    <body>


        <ul class="footer-tip">
            <li><h1>克隆成功！</h1></li>
            <li>
                <a id="link_goon">继续克隆</a>
                <a onclick="redirectManager(1);">返回出售管理</a>
                <a onclick="redirectManager(3);">返回出租管理</a>
            </li>
        </ul>
        <script src="~/Scripts/jquery-1.8.3.js"></script>
        <script type="text/javascript">
	    $(document).ready(function() {
	        if(window.parent)
	        {
	            window.parent.loadingHide();
	        }
	    });

	    $(function() {
	        $("#link_goon").click(function() {
	            if(window.parent.frames[0])
	            {
	                window.parent.frames[0].getShareBuildings();
	            }

	        });
	    });

	    function redirectManager(postType) {
	        if (postType == 1) {
	            id = "houseManagerSell";
	        } else {
	            id = "houseManagerRent";
	        }
	        parent.parent.$("#" + id).click();
	        var href = parent.parent.$("#" + id).attr("href");
	        var url = href.substring(href.indexOf("'") + 1, href.lastIndexOf("'"));
	        url += "&buildingType=1&isDraft=0";
	        parent.parent.$("#" + id).parents(".menu").addClass("menu_open");
	        parent.parent.$("#" + id).parents(".menu").siblings().removeClass("menu_open");
	        parent.parent.hrefLink(url);
	    }
        </script>

    </body>

}
