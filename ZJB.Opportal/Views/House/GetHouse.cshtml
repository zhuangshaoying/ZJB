@using ZJB.Api.Common
@using ZJB.Api.Models
@using ZJB.Core.Utilities
@using ZJB.WX.Models
@using ZJB.Api.Entity;
@using ZJB.Api.Models.Community
@{
    Layout = null;
    int posttype = ViewBag.Posttype;
    int editType = ViewBag.EditType;
    int buildType = ViewBag.BuildingType;
    int chouseId = ViewBag.ChouseId;
    int isChange = chouseId > 0 ? 1 : 0;
    int closeTip = chouseId > 0 ? 1 : 0;
    int isCloned = ViewBag.ShowColned;///如果是克隆成功 则显示相应页面
    switch (posttype)
    {
        case 1:
            ViewBag.Title = "出售房源录入";
            break;
        case 2:
            ViewBag.Title = "求购房源录入";
            break;
        case 3:
            ViewBag.Title = "出租房源录入";
            break;
        case 4:
            ViewBag.Title = "求租房源录入";
            break;
        default:
            ViewBag.Title = "出售房源录入";
            break;
    }

}
@model  ZJB.Opportal.Models.HouseModels
@{
    var itemHouseBasicInfo = Model.HouseBasicInfo;
    var itemHouseInfo = Model.HouseInfo;
    var itemVillaInfo = Model.VillaInfo;
    var itemShopInfo = Model.ShopInfo;
    var itemOfficeInfo = Model.OfficeInfo;
    var itemFactoryInfo = Model.FactoryInfo;
    var itemHouseImages = Model.HouseImages;
    var regions = (CommunityDetailModel)ViewBag.RegionsModel;
    if (regions == null)
    {
        regions = new CommunityDetailModel();
    }
    var selected = "";
    System.Web.Script.Serialization.JavaScriptSerializer jss = new System.Web.Script.Serialization.JavaScriptSerializer();
}
<!DOCTYPE HTML PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
